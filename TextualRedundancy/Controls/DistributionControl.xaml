<UserControl x:Class="TextualRedundancy.Controls.DistributionControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TextualRedundancy.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid x:Name="MainGrid">
        <Border x:Name="GraphBorder" BorderBrush="#A0A0A0" BorderThickness="0" Margin="8" Padding="10">
            <Image x:Name="MainImage" Stretch="Fill"/>
        </Border>
    </Grid>
</UserControl>
